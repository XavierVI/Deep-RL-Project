{
    "seed": 42,
    "env_name": "BipedalWalker-v3",
    "action_space_type": "continuous",
    "display": true,
    "display_episodes":100,
    "num_episodes": 3000,
    "max_steps": 500,
    "hidden_layers": [128, 128],
    "activation_function": "relu",
    "learning_rate": 0.001,
    "gamma": 0.99,

    "algorithm": "reinforce",
    
    "reinforce_initial_epsilon": 1.0,
    "reinforce_min_epsilon": 0.001,
    "reinforce_decay_rate": 0.01,
    "reinforce_initial_tau": 2.0,
    "reinforce_min_tau": 0.1,
    "reinforce_decay_rate_tau": 0.001,
    "reinforce_use_baseline": true,
    "reinforce_use_boltzmann": true,

    "sarsa_initial_epsilon": 1.0,
    "sarsa_min_epsilon": 0.01,
    "sarsa_decay_rate": 0.001,
    "sarsa_initial_tau": 2.0,
    "sarsa_min_tau": 0.1,
    "sarsa_decay_rate_tau": 0.001,
    "sarsa_use_boltzmann": true,

    "learning_rates": [0.0001, 0.001, 0.01],
    "gammas": [0.90, 0.95, 0.99],
    "boltzmann_options": [true, false]
}